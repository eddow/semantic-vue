{
	"name": "v-semantic",
	"version": "2.0.1",
	"description": "Semantic-ui2 + vue2",
	"main": "dist/v-semantic.js",
	"ts:main": "src/index.ts",
	"sideEffects": false,
	"typescript": {
		"definition": "dist/v-semantic.d.ts"
	},
	"directories": {
		"example": "demo",
		"doc": "docs",
		"lib": "dist"
	},
	"scripts": {
		"start": "webpack --watch",
		"demo": "webpack --watch --config demo/webpack.config.js",
		"prepack": "webpack",
		"lint": "eslint src"
	},
	"keywords": [
		"semantic",
		"semantic-ui",
		"vue"
	],
	"repository": "github:eddow/v-semantic.git",
	"author": "fmdm",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/eddow/v-semantic/issues"
	},
	"config": {
		"semantic": {
			"install": true,
			"src": "./node_modules/semantic-ui/src/",
			"config": "./src/less/semantic/theme.config"
		}
	},
	"homepage": "https://github.com/eddow/v-semantic#readme",
	"devDependencies": {
		"@types/jquery": "^3.3.22",
		"@types/semantic-ui": "^2.2.6",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.6",
		"css-loader": "^1.0.1",
		"eslint": "^4.19.1",
		"eslint-config-standard": "^11.0.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-node": "^6.0.1",
		"eslint-plugin-promise": "^3.8.0",
		"eslint-plugin-standard": "^3.1.0",
		"eslint-plugin-vue": "^4.7.1",
		"expose-loader": "^0.7.5",
		"file-loader": "^2.0.0",
		"fuse-box": "^3.6.0",
		"raw-loader": "^0.5.1",
		"semantic-ui": "^2.4.2",
		"source-map-loader": "^0.2.4",
		"style-loader": "^0.23.1",
		"ts-json-schema-decorator": "^1.2.4",
		"ts-loader": "^5.3.0",
		"typescript": "^3.1.6",
		"vue": "^2.6.10",
		"vue-codemirror": "^4.0.6",
		"vue-hot-reload-api": "^2.2.0",
		"vue-loader": "^15.4.2",
		"vue-property-decorator": "^6.1.0",
		"vue-router": "^3.0.1",
		"vue-template-compiler": "^2.6.10",
		"vue-template-es2015-compiler": "^1.6.0",
		"webpack": "^4.25.1",
		"webpack-cli": "^3.1.2",
		"webpack-dts-bundle": "0.0.2",
		"webpack-node-externals": "^1.7.2"
	},
	"dependencies": {
		"ajv": "^6.5.5",
		"node-sass": "^4.5.3",
		"tslib": "^1.9.3",
		"vue-resize-directive": "^1.1.4",
		"vue-ripper": "^1.1.9"
	},
	"peerDependencies": {
		"jquery": "^3.3.1",
		"semantic-ui": "^2.4",
		"vue": "^2.6.0"
	}
}
